{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "ui:dev",
      "type": "shell",
      "isBackground": true,
      "command": "npm",
      "args": ["run", "dev"],
      "options": {
        "env": {
          "BROWSER": "none"
        }
      },
      "problemMatcher": {
        "pattern": {
          "regexp": "."
        },
        "background": {
          "beginsPattern": ".*",
          "endsPattern": "(Local|ready|running at)"
        }
      }
    }
  ]
}